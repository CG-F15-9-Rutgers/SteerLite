	<zmin>85</zmin>
	<zmax>86</zmax>
	<ymin>0</ymin>
	<ymax>1</ymax>
	<xmin>-5</xmin>
	<xmax>-3</xmax>
  </obstacle>

  <!-- Bus B - Rear Bottleneck - Far side -->
  <obstacle>
	<zmin>85</zmin>
	<zmax>86</zmax>
	<ymin>0</ymin>
	<ymax>1</ymax>
	<xmin>-11</xmin>
	<xmax>-9</xmax>
  </obstacle>

  <!-- Bus B - Close Side - Rear -->
  <obstacle>
    <xmin>-3</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>85</zmin>
    <zmax>90</zmax>
  </obstacle>

  <!-- Bus Stop - Close Wall -->
  <obstacle>
    <xmin>20</xmin>
    <xmax>21</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>-10</zmin>
    <zmax>1</zmax>
  </obstacle>
  
  <!-- Bus Stop - Bottom Wall -->
  <obstacle>
    <xmin>20</xmin>
    <xmax>100</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>10</zmin>
    <zmax>11</zmax>
  </obstacle>

  <!-- Bus Stop - Top Wall -->
  <obstacle>
    <xmin>20</xmin>
    <xmax>100</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>-11</zmin>
    <zmax>-10</zmax>
  </obstacle>
  

  <!-- Stop to A travelers -->
  <agentRegion>
    <numAgents>10</numAgents>
    <regionBounds>
      <xmin>13</xmin>
      <xmax>35</xmax>
      <ymin>0</ymin>
      <ymax>0</ymax>
      <zmin>-10</zmin>
      <zmax>10</zmax>
    </regionBounds>
    <initialConditions>
      <direction> <random>true</random> </direction>
      <radius>0.8</radius>
      <speed>0</speed>
    </initialConditions>
    <goalSequence>
      <seekStaticTarget>
        <targetLocation>
          <x>-7</x>
          <y>0</y>
          <z>-4</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
    </goalSequence>
  </agentRegion>

  <!-- A to Stop travelers -->
  <agentRegion>
    <numAgents>20</numAgents>
    <regionBounds>
      <xmin>-11</xmin>
      <xmax>-2</xmax>
      <ymin>0</ymin>
      <ymax>0</ymax>
      <zmin>-16</zmin>
      <zmax>2</zmax>
    </regionBounds>
    <initialConditions>
      <direction> <random>true</random> </direction>
      <radius>0.8</radius>
      <speed>0</speed>
    </initialConditions>
    <goalSequence>
      <seekStaticTarget>
        <targetLocation>
