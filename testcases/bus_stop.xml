<SteerBenchTestCase xmlns="http://www.magix.ucla.edu/steerbench"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://www.magix.ucla.edu/steerbench 
                                        TestCaseSchema.xsd">

  <header>
    <version>1.0</version>
    <name>bus-stop</name>
    <worldBounds>
      <xmin>-30</xmin>
      <xmax>50</xmax>
      <ymin>0</ymin>
      <ymax>0</ymax>
      <zmin>-100</zmin>
      <zmax>100</zmax>
    </worldBounds>
  </header>
  
  <suggestedCameraView>
    <position> <x>40</x> <y>80</y> <z>100</z> </position>
    <lookat> <x>20</x> <y>0</y> <z>40</z> </lookat>
	<up> <x>0</x> <y>1</y> <z>0</z> </up>
    <fovy>45</fovy>
  </suggestedCameraView>
  
  <!-- Bus A - Front -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>-20</zmin>
    <zmax>-16</zmax>
  </obstacle>

  <!-- Bus A - Rear -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>7</zmin>
    <zmax>10</zmax>
  </obstacle>

  <!-- Bus A - Far Side -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-11</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>-16</zmin>
    <zmax>7</zmax>
  </obstacle>
  
  <!-- Bus A - Close Side - Mid -->
  <obstacle>
    <xmin>-3</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>-13</zmin>
    <zmax>-5</zmax>
  </obstacle>

  <!-- Bus A - Close Side - Rear -->
  <obstacle>
    <xmin>-3</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>0</zmin>
    <zmax>7</zmax>
  </obstacle>

  <!-- Bus A - Rear Bottleneck - Close side -->
  <obstacle>
	<zmin>0</zmin>
	<zmax>1</zmax>
	<ymin>0</ymin>
	<ymax>1</ymax>
	<xmin>-5</xmin>
	<xmax>-3</xmax>
  </obstacle>

  <!-- Bus A - Rear Bottleneck - Far side -->
  <obstacle>
	<zmin>0</zmin>
	<zmax>1</zmax>
	<ymin>0</ymin>
	<ymax>1</ymax>
	<xmin>-11</xmin>
	<xmax>-9</xmax>
  </obstacle>

  <!-- Bus B - Front -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>50</zmin>
    <zmax>54</zmax>
  </obstacle>

  <!-- Bus B - Rear -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-2</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>90</zmin>
    <zmax>93</zmax>
  </obstacle>

  <!-- Bus B - Far Side -->
  <obstacle>
    <xmin>-12</xmin>
    <xmax>-11</xmax>
    <ymin>0</ymin>
    <ymax>1</ymax>
    <zmin>54</zmin>
    <zmax>90</zmax>
  </obstacle>
  
  <!-- Bus B - Close Side - Front-Mid -->
  <obstacle>
    <xmin>-3</xmin>
    <xmax>-2</xmax>
