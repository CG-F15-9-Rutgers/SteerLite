    <numAgents>65</numAgents>
    <regionBounds>
      <xmin>13</xmin>
      <xmax>35</xmax>
      <ymin>0</ymin>
      <ymax>0</ymax>
      <zmin>-10</zmin>
      <zmax>10</zmax>
    </regionBounds>
    <initialConditions>
      <direction> <random>true</random> </direction>
      <radius>0.8</radius>
      <speed>0</speed>
    </initialConditions>
    <goalSequence>
      <!-- seekStaticTarget>
        <targetLocation>
          <x>11</x>
          <y>0</y>
          <z>10</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget -->
      <seekStaticTarget>
        <targetLocation>
          <x>5</x>
          <y>0</y>
          <z>54</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
      <seekStaticTarget>
        <targetLocation>
          <x>-7</x>
          <y>0</y>
          <z>72</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
    </goalSequence>
  </agentRegion>

  <!-- B to Stop travelers -->
  <agentRegion>
    <numAgents>70</numAgents>
    <regionBounds>
      <xmin>-11</xmin>
      <xmax>-2</xmax>
      <ymin>0</ymin>
      <ymax>0</ymax>
      <zmin>54</zmin>
      <zmax>90</zmax>
    </regionBounds>
    <initialConditions>
      <direction> <random>true</random> </direction>
      <radius>0.8</radius>
      <speed>0</speed>
    </initialConditions>
    <goalSequence>
      <seekStaticTarget>
        <targetLocation>
          <x>0</x>
          <y>0</y>
          <z>54</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
      <seekStaticTarget>
        <targetLocation>
          <x>0</x>
          <y>0</y>
          <z>5</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
      <seekStaticTarget>
        <targetLocation>
          <x>20</x>
          <y>0</y>
          <z>5</z>
		</targetLocation>
        <desiredSpeed>1.3</desiredSpeed>
        <timeDuration>1000.0</timeDuration>
      </seekStaticTarget>
    </goalSequence>
